<Window x:Class="DICOMConverter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:eo="http://schemas.essentialobjects.com/wpf/"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DICOMConverter"
        mc:Ignorable="d"
        Title="DICOMConverter" Height="236.364" Width="500">
    <Grid Margin="0,0,0,12">
        <Button x:Name="buttonOpenFolder" Content="open files" HorizontalAlignment="Left" 
                Margin="208,58,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="2.649,-1.143" Click="ButtonOpenFolder_Click"/>
        <eo:RangeSlider x:Name="sliderIntensity" Minimum="0" Maximum="500" 
                        TickFrequency="50" TickPlacement="TopLeft" Margin="54,10,54,140"
                        RangeStart="235" RangeEnd="245" RenderTransformOrigin="0.5,0.5">

        </eo:RangeSlider>
        <TextBox x:Name="textBoxLogs" HorizontalAlignment="Left" Height="84" Margin="10,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="472"/>
    </Grid>
</Window>
